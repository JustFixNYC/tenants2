	IF Tenant address zip TE = &quot;10035&quot; OR Tenant address zip TE = &quot;10037&quot; OR (Tenant address zip TE = &quot;10037&quot;) //AND NYCHA
		DEFAULT Court location MC TO &quot;Harlem Community Justice Center&quot;
