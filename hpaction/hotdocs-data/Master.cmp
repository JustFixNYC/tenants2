END IF
IF Tenant borough MC = &quot;Staten Island&quot;
	DEFAULT Court location MC TO &quot;Richmond County&quot;
END IF


IF Court location MC = &quot;Bronx&quot;
	SET Court county MC TO &quot;Bronx&quot;
ELSE IF Court location MC = &quot;Harlem Community Justice Center&quot; OR Court location MC = &quot;New York County&quot;
	SET Court county MC TO &quot;New York&quot;
ELSE IF Court location MC = &quot;Kings County&quot; OR Court location MC = &quot;Redhook Community Justice Center&quot;
	SET Court county MC TO &quot;Kings&quot;
ELSE IF Court location MC = &quot;Queens County&quot;
	SET Court county MC TO &quot;Queens&quot;
