{
  "name": "tenants2",
  "version": "0.0.1",
  "description": "JustFix tenants app number two",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "sass": "node-sass frontend/sass/styles.scss frontend/static/frontend/styles.css --source-map frontend/static/frontend/styles.css.map --source-map-contents",
    "sass:watch": "npm run sass -- --watch",
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "npm run sass && concurrently --kill-others \"npm run watch\" \"npm run sass:watch\"",
    "postinstall": "npm run sass && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justfixnyc/tenants2.git"
  },
  "author": "Atul Varma <atul@justfix.nyc>",
  "license": "ISC",
  "engines": {
    "node": ">=8.4.0"
  },
  "bugs": {
    "url": "https://github.com/justfixnyc/tenants2/issues"
  },
  "homepage": "https://github.com/justfixnyc/tenants2#readme",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/webpack": "^4.4.9",
    "apollo": "^1.6.0",
    "concurrently": "^3.6.1",
    "jest": "^23.4.2",
    "ts-jest": "^23.1.3",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "dependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "@types/node": "^10.5.7",
    "@types/react": "^16.4.8",
    "@types/react-dom": "^16.0.7",
    "autobind-decorator": "^2.1.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bulma": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.9.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
