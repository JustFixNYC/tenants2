{
  "name": "tenants2",
  "version": "0.0.1",
  "description": "JustFix tenants app number two",
  "main": "index.js",
  "scripts": {
    "lint": "tsc --noEmit",
    "test": "jest && npm run lint",
    "test:watch": "jest --watch",
    "sass": "node-sass frontend/sass/styles.scss frontend/static/frontend/styles.css --source-map frontend/static/frontend/styles.css.map --source-map-contents --output-style compressed",
    "sass:watch": "npm run sass -- --watch",
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "npm run sass && concurrently --kill-others \"npm run watch\" \"npm run sass:watch\"",
    "postinstall": "npm run sass && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justfixnyc/tenants2.git"
  },
